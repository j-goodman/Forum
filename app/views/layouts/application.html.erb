<!DOCTYPE html>
<html>
<head>
  <title>OOForum</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if signed_in? %>
  <%= current_user.username %>
  <%= button_to "Log Out", session_url, method: :delete %>
<% end %>

<%= yield %>

</body>
</html>
